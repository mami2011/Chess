
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Hibernate session factory -->
	<bean id="fileDao" class="com.vendertool.fps.dal.file.FileDaoImpl">
		<property name="sessionFactory" ref="batchdbSessionFactory"></property>
	</bean>
	
	<bean id="jobDao" class="com.vendertool.fps.dal.job.JobDaoImpl">
		<property name="sessionFactory" ref="batchdbSessionFactory"></property>
	</bean>
	<bean id="taskDao" class="com.vendertool.fps.dal.task.TaskDaoImpl">
		<property name="sessionFactory" ref="batchdbSessionFactory"></property>
	</bean>
	<bean id="sequenceDataDao" class="com.vendertool.fps.dal.sequenceData.SequenceDataDaoImpl">
		<property name="sessionFactory" ref="batchdbSessionFactory"></property>
	</bean>
</beans>