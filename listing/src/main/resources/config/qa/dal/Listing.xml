
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Hibernate session factory -->
	<bean id="listingDao" class="com.vendertool.listing.dal.listing.ListingDaoImpl">
		<property name="sessionFactory" ref="invdbSessionFactory"></property>
	</bean>
	
	<bean id="listingVariationDao" class="com.vendertool.listing.dal.listing.ListingVariationDaoImpl">
		<property name="sessionFactory" ref="invdbSessionFactory"></property>
	</bean>
</beans>