<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="image" match-catalog="invdb"></table-filter>
	<table-filter match-name="listing" match-catalog="invdb"></table-filter>
	<table-filter match-name="listing_variation"
		match-catalog="invdb">
	</table-filter>
	<table-filter match-name="product" match-catalog="invdb"></table-filter>
	<table-filter match-name="product_attribute"
		match-catalog="invdb">
	</table-filter>
	<table-filter match-name="product_variation"
		match-catalog="invdb">
	</table-filter>
</hibernate-reverse-engineering>